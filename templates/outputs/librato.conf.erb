# Forward metrics to Librato:
[[outputs.librato]]

  # Librato API user:
  api_user = "<%= @api_user %>"

  # Librato API token:
  api_token = "<%= @api_token %>"

<% if @source_tag -%>
  # Tag Field to populate source attribute (optional)
  # This is typically the _hostname_ from which the metric was obtained.
  source_tag = "<%= @source_tag %>"

<% end -%>
<% if @timeout -%>
  # Connection timeout:
  timeout = "<%= @timeout %>"

<% end -%>